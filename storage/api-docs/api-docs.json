{
    "openapi": "3.0.0",
    "info": {
        "title": "Website Socav RDC API",
        "version": "0.1"
    },
    "paths": {
        "/api/auth/login": {
            "post": {
                "tags": [
                    "Authenticate"
                ],
                "summary": "Create",
                "description": "Creation",
                "operationId": "login",
                "requestBody": {
                    "description": "Enregistrer",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "winner@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "12345"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "success"
                    },
                    "401": {
                        "description": "ysteme"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/auth/logout": {
            "get": {
                "tags": [
                    "Authenticate"
                ],
                "summary": "Deconnexion",
                "description": "Deconnexion d'un utilisateur en utilisant son token",
                "operationId": "logout",
                "responses": {
                    "200": {
                        "description": "Vous etes deconecte",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "example": "success"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Vous etes deconecte"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/createUSer": {
            "post": {
                "tags": [
                    "User"
                ],
                "summary": "Create",
                "description": "Creation",
                "operationId": "storeUser",
                "requestBody": {
                    "description": "Enregistrer",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "phone",
                                    "password"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "winner kambale"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "mpesa"
                                    },
                                    "phone": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "2024-09-19"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "2024-09-19"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "success"
                    },
                    "401": {
                        "description": "ysteme"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/updateUser/{id}": {
            "put": {
                "tags": [
                    "User"
                ],
                "summary": "Update",
                "description": "Modification",
                "operationId": "updateUser",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "description": "Modifier",
                    "required": false,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "phone",
                                    "password"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "winner kambale"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "mpesa"
                                    },
                                    "phone": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "2024-09-19"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "2024-09-19"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "success"
                    },
                    "404": {
                        "description": "User not found"
                    },
                    "422": {
                        "description": "Validation error"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/deleteUser/{id}": {
            "delete": {
                "tags": [
                    "User"
                ],
                "summary": "Delete",
                "description": "Suppression",
                "operationId": "deleteUser",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "User deleted successfully"
                    },
                    "404": {
                        "description": "User not found"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/getAllUser": {
            "get": {
                "tags": [
                    "User"
                ],
                "summary": "Get",
                "description": "Returns list",
                "operationId": "getUsers",
                "responses": {
                    "200": {
                        "description": "Successful",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "name": "Authorization",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "Bearer"
            }
        }
    },
    "tags": [
        {
            "name": "Authenticate",
            "description": "Authenticate"
        },
        {
            "name": "User",
            "description": "User"
        }
    ]
}